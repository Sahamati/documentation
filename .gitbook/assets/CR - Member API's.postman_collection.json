	"item": [
		{
			"name": "Member Token",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Generate member token\", function() {",
							"    pm.response.to.have.status(200);",
							"})",
							"",
							"if(pm.response.code == 200){",
							"    pm.environment.set(\"member_token\",pm.response.json().accessToken);",
							"}"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/x-www-form-urlencoded"
					}
				],
				"body": {
					"mode": "urlencoded",
					"urlencoded": [
						{
							"key": "id",
							"value": "{{member_id}}",
							"type": "text"
						},
						{
							"key": "secret",
							"value": "{{member_secret}}",
							"type": "text"
						}
					]
				},
				"url": "{{host}}/iam/v1/entity/token/generate"
			},
			"response": []
		},
		{
			"name": "FIP",
			"request": {
				"auth": {
					"type": "bearer",
					"bearer": {
						"token": "{{member_token}}"
					}
				},
				"method": "GET",
				"header": [],
				"url": "{{host}}/cr/v2/entityInfo/FIP"
			},
			"response": []
		},
		{
			"name": "AA",
